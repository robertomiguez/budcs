    "test:unit": "vue-cli-service test:unit --passWithNoTests",
    "lint": "vue-cli-service lint",
    "gh-pages-deploy": "node scripts/gh-pages-deploy.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chart.js": "^2.9.3",
    "core-js": "^3.6.5",
